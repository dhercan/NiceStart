        android:startOffset="950"
        android:duration="350"
        android:fillAfter="true"
        android:fromYDelta="0"
        android:toYDelta="-30%p"

        />
    <translate
        android:startOffset="1300"
        android:duration="380"
        android:fillAfter="true"
        android:fromYDelta="0"
        android:toYDelta="30%p"

        />

    <translate
        android:startOffset="1680"
        android:duration="270"
        android:fillAfter="true"
        android:fromYDelta="0"
        android:toYDelta="-18%p"

        />
    <translate
        android:startOffset="1950"
        android:duration="300"
        android:fillAfter="true"
        android:fromYDelta="0"
        android:toYDelta="18%p"

        />

    <translate
        android:startOffset="2250"
        android:duration="200"
        android:fillAfter="true"
        android:fromYDelta="0"
        android:toYDelta="-10%p"

        />

    <translate
        android:startOffset="2450"
        android:duration="230"
        android:fillAfter="true"
        android:fromYDelta="0"
        android:toYDelta="10%p"

        />

</set>